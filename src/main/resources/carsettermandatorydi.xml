<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"https://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans >
<bean id="car" class="com.workshop.springdemo.beans.CarSetterMandatory" dependency-check="all">
<property name="name" value="Mercedes-Benz"/>
 <property name="engine" ref="engine" > 
 </property>
</bean>
<bean id="engine" class="com.workshop.springdemo.beans.Engine">
<property name="modelYear" value="2015"/>
</bean>
</beans>